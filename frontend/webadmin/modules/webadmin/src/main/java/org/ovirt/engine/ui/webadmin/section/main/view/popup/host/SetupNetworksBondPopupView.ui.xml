<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
    xmlns:g="urn:import:com.google.gwt.user.client.ui"
    xmlns:d="urn:import:org.ovirt.engine.ui.common.widget.dialog"
    xmlns:e="urn:import:org.ovirt.engine.ui.common.widget.editor"
    xmlns:ge="urn:import:org.ovirt.engine.ui.common.widget.editor.generic"
    xmlns:b="urn:import:org.gwtbootstrap3.client.ui">

    <d:SimpleDialogPanel width="400px" height="190px">
        <d:content>
            <b:Container>
                <b:Row>
                    <ge:ListModelSuggestBoxEditor ui:field="bondSuggestEditor" addStyleNames="hbpv_bondSuggestEditor_pfly_fix" />
                </b:Row>
                <b:Row>
                    <e:ListModelListBoxEditor ui:field="bondingModeEditor" />
                </b:Row>
                <b:Row>
                    <ge:StringEntityModelTextBoxEditor ui:field="customBondEditor" enabled="false" />
                </b:Row>
            </b:Container>
        </d:content>
    </d:SimpleDialogPanel>

</ui:UiBinder>
