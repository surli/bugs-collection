<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
    xmlns:g="urn:import:com.google.gwt.user.client.ui"
    xmlns:ge="urn:import:org.ovirt.engine.ui.common.widget.editor.generic"
    xmlns:b="urn:import:org.gwtbootstrap3.client.ui">

    <ui:style type="org.ovirt.engine.ui.webadmin.section.main.view.popup.macpool.MacRangeEditor.Style">
        .elementStyle {
            float: left;
            padding-right: 10px;
        }

        .floatLeft {
            float: left;
        }

        .wrapper {
            padding-bottom: 10px;
        }
    </ui:style>

    <g:FlowPanel addStyleNames="{style.wrapper}">
        <ge:StringEntityModelTextBoxEditor ui:field="leftBound" addStyleNames="{style.floatLeft}" usePatternFly="true" removeFormGroup="true"/>
        <ge:StringEntityModelTextBoxEditor ui:field="rightBound" addStyleNames="{style.floatLeft}" usePatternFly="true" removeFormGroup="true" />
    </g:FlowPanel>

</ui:UiBinder>