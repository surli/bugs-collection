<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
    xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:d="urn:import:org.ovirt.engine.ui.common.widget.dialog"
    xmlns:ge="urn:import:org.ovirt.engine.ui.common.widget.editor.generic"
    xmlns:p="urn:import:org.ovirt.engine.ui.webadmin.widget.provider"
    xmlns:b="urn:import:org.gwtbootstrap3.client.ui" >

    <ui:style>
        .container {
            width: 100%;
        }
        .firstRow {
            padding-top: 15px;
        }
    </ui:style>

    <d:SimpleDialogPanel width="430px" height="400px">
        <d:content>
            <g:ScrollPanel>
                <b:Container addStyleNames="{style.container}">
                    <ge:StringEntityModelLabelEditor ui:field="networkEditor" />
                    <p:ExternalSubnetWidget ui:field="subnetWidget" />
                </b:Container>
            </g:ScrollPanel>
        </d:content>
    </d:SimpleDialogPanel>

</ui:UiBinder>

