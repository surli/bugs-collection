#%RAML 0.8
baseUri: https://api.example.com
title: Filesystem API
version: 0.1
mediaType:  application/json
/files:
  get:
    displayName: Create file
    description: Create file.
    queryParameters:
      notArray:
        type: string
        required: true
        repeat: false
      stringArray:
        type: string
        required: true
        repeat: true
    responses:
      201:
        description: file was created.
        headers:
          Location:
            type: string
            required: true
            example: /file/bd2907ea-5f86-11e5-9d70-feff819cdc9f
        body:
          application/json: